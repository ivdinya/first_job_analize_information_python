#проверка является ли число простым
def is_prime(n):
    if n <= 1:
        return False
    if n == 2:
        return True
    if n % 2 == 0:
        return False
    for i in range(3, int(n**0.5)+1, 2):
        if n % i == 0:
            return False
    return True

def blizko(b):
    if b <= 0:
        return b
    if b <= 1:
        return 2

    lower = b
    up = b

    while True:
        if is_prime(lower):
            return lower
        if is_prime(up):
            return up
        lower -= 1
        up += 1



a = input("Введите свое число: ")
b = int(a)
c = blizko(b)

print(a,c)
